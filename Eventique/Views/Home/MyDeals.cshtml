
@{
    ViewData["Title"] = "MyDeals";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}


@section Styles {
    <link rel="icon" type="image/png" href="~/Images/newLogo.png" />
    <link rel="stylesheet" type="text/css" href="~/pato/fonts/themify/themify-icons.css">
    <link rel="stylesheet" type="text/css" href="~/pato/vendor/select2/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/pato/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/pato/css/main.css">
    <link rel="stylesheet" href="~/myDeals/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/elegant-icons.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/style.css" type="text/css">
    <!-- Stylesheets -->
}

<section class="breadcrumb-section set-bg" data-setbg="img/breadcrumb.jpg" style="position:relative;margin-top:400px">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="breadcrumb__text">
                    <h2>Shopping Cart</h2>
                    <div class="breadcrumb__option">
                        <a href="./index.html">Home</a>
                        <span>Shopping Cart</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@*Breadcrumb Section End
    Shoping Cart Section Begin*@
<section class="shoping-cart spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="">
                    @{
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">Deal with</th>
                                    <th scope="col">Deal Date</th>
                                    <th scope="col">Deal Price</th>
                                    <th scope="col">Deal Message</th>
                                    <th scope="col">Deal Status</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in ViewData["phoRequest"] as IEnumerable<Eventique.Models.PhotographerRequest>)
                                {
                                    <tr>
                                        <td>
                                            <img src="" style="width:50px;height:50px" alt="">
                                            <h5>@item.RequestPhotographer.Ph_Name</h5>
                                        </td>
                                        <td>
                                            <date>@item.Date</date>
                                        </td>
                                        <td>
                                            @item.RequestPhotographer.Ph_Price
                                        </td>
                                        <td>
                                            @item.Message
                                        </td>
                                        <td>
                                            @item.Status
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                </div>
            </div>
        </div>
    </div>
</section>

<section class="shoping-cart spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="">
                    @{
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">Deal with</th>
                                    <th scope="col">Deal Date</th>
                                    <th scope="col">Deal Price</th>
                                    <th scope="col">Deal Message</th>
                                    <th scope="col">Deal Status</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item2 in ViewData["WeddRequest"] as IEnumerable<Eventique.Models.WeddingHallsRequest>)
                                {
                                    <tr>
                                        <td>
                                            <img src="" style="width:50px;height:50px" alt="">
                                            <h5>@item2.RequestHotel.Name</h5>
                                        </td>
                                        <td>
                                            <date>@item2.Date</date>
                                        </td>
                                        <td>
                                            @item2.RequestHotel.Hall_Price
                                        </td>
                                        <td>
                                            @item2.Message
                                        </td>
                                        <td>
                                            @item2.Status
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Shoping Cart Section End -->

@section Scripts {
    <script src="~/myDeals/js/jquery-3.3.1.min.js"></script>
    <script src="~/myDeals/js/bootstrap.min.js"></script>
    <script src="~/myDeals/js/jquery.nice-select.min.js"></script>
    <script src="~/myDeals/js/jquery-ui.min.js"></script>
    <script src="~/myDeals/js/jquery.slicknav.js"></script>
    <script src="~/myDeals/js/mixitup.min.js"></script>
    <script src="~/myDeals/js/owl.carousel.min.js"></script>
    <script src="~/myDeals/js/main.js"></script>
}
