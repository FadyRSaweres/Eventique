
@{
    ViewData["Title"] = "MyDeals";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}


@section Styles {
    <link rel="icon" type="image/png" href="~/Images/newLogo.png" />
    <link rel="stylesheet" type="text/css" href="~/pato/fonts/themify/themify-icons.css">
    <link rel="stylesheet" type="text/css" href="~/pato/vendor/select2/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/pato/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/pato/css/main.css">
    <link rel="stylesheet" href="~/myDeals/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/elegant-icons.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/myDeals/css/slicknav.min.css" type="text/css">
    @*<link rel="stylesheet" href="~/myDeals/css/style.css" type="text/css">*@
    <link rel="stylesheet" href="~/assets/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="~/assets/css/style.css" />
    <link href="~/css/AllWeddingHallsStyle.css" rel="stylesheet" />
    <!-- Stylesheets -->
}

<section class="breadcrumb-section set-bg" data-setbg="img/breadcrumb.jpg" style="position:relative;margin-top:250px">
    <div class="breadcrumb-area bg-img bg-overlay jarallax" style="background-image: url(/Images/pic20.jpg);">
        <div class="col-md-8 offset-md-2 mt-5" style="">
        </div>
        <div class="container h-100">
            <div class="row h-100 align-items-center">
                <div class="col-12">
                    <div class="breadcrumb-content text-center">
                        <h2 class="page-title">My Deals</h2>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb justify-content-center">
                                <li class="breadcrumb-item"><a href="">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">My Deals</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="row justify-content-center mb-5 pb-2" style="margin-top:100px;">
    <div class="col-lg-8">
        <div class="card">
            <div class="card-header">
                <strong class="card-title">Deals With Photographers</strong>
            </div>
            <div class="table-stats order-table ov-h">
                <table class="table ">
                    <thead>
                        <tr>
                            <th class="serial">Request ID</th>
                            <th class="avatar">Avatar</th>
                            <th>Name</th>
                            <th>Date</th>
                            <th>Price</th>
                            <th>Message</th>
                            <th>Request Time</th>
                            <th>Status</th>
                        </tr>
                    </thead>
                    <tbody>

                        @{ foreach (var item in ViewData["phoRequest"] as List<Eventique.Models.PhotographerRequest>)
                            {
                                <tr>
                                    <td class="serial">@item.ID</td>
                                    <td class="avatar">
                                        <div class="round-img">
                                            <a href="#"><img class="rounded-circle" src="@item.RequestPhotographer.ImagePath" alt=""></a>
                                        </div>
                                    </td>
                                    <td>@item.RequestPhotographer.Ph_Name</td>
                                    <td>  <span class="name">@item.Date</span> </td>
                                    <td> <span class="product">@item.RequestPhotographer.Ph_Price $</span> </td>
                                    <td><span class="count">@item.Message</span></td>
                                    <td><span class="count">@item.Time</span></td>
                                    <td>
                                        <span class="badge badge-complete">@item.Status</span>
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<div class="row justify-content-center mb-5 pb-2">
    <div class="col-lg-8">
        <div class="card">
            <div class="card-header">
                <strong class="card-title">Deals With Wedding Halls</strong>
            </div>
            <div class="table-stats order-table ov-h">
                <table class="table ">
                    <thead>
                        <tr>
                            <th class="serial">Request ID</th>
                            <th class="avatar">Avatar</th>
                            <th>Name</th>
                            <th>Date</th>
                            <th>Offer Title</th>
                            <th>Price</th>
                            <th>Message</th>
                            <th>Request Time</th>
                            <th>Status</th>
                        </tr>
                    </thead>
                    <tbody>

                        @{ foreach (var item in ViewData["WeddRequest"] as List<Eventique.Models.WeddingHallsRequest>)
                            {
                                <tr>
                                    <td class="serial">@item.ID</td>
                                    <td class="avatar">
                                        <div class="round-img">
                                            <a href="#"><img class="rounded-circle" src="@item.RequestHotel.Hall_ImgPath" alt=""></a>
                                        </div>
                                    </td>
                                    <td>@item.RequestHotel.Name</td>
                                    <td>  <span class="name">@item.Date</span> </td>
                                    <td>  <span class="name">@item.Offer.Title</span> </td>
                                    <td> <span class="product">@item.Offer.Price $</span> </td>
                                    <td><span class="count">@item.Message</span></td>
                                    <td><span class="count">@item.Time</span></td>

                                    <td>
                                        <span class="badge badge-complete">@item.Status</span>
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<div class="row justify-content-center mb-5 pb-2">
    <div class="col-lg-8">
        <div class="card">
            <div class="card-header">
                <strong class="card-title">Deals With Photographers</strong>
            </div>
            <div class="table-stats order-table ov-h">
                <table class="table ">
                    <thead>
                        <tr>
                            <th class="serial">Request ID</th>
                            <th class="avatar">Avatar</th>
                            <th>Name</th>
                            <th>Date</th>
                            <th>Quantitiy</th>
                            <th>Price</th>
                            <th>Message</th>
                            <th>Request Time</th>
                            <th>Invition Style</th>
                            <th>Status</th>
                        </tr>
                    </thead>
                    <tbody>

                        @{ foreach (var item in ViewData["DesiRequest"] as List<Eventique.Models.DesignerRequest>)
                            {                        
                                <tr>
                                    <td class="serial">@item.ID</td>
                                    <td class="avatar">
                                        <div class="round-img">
                                            <a href="#"><img class="rounded-circle" src="@item.RequestHotel.Designer_ImgPath" alt=""></a>
                                        </div>
                                    </td>
                                    <td>@item.RequestHotel.ShopName</td>
                                    <td>  <span class="name">@item.Date</span> </td>
                                    <td>@item.Quantity</td>
                                    <td> <span class="product">@item.price() $</span> </td>
                                    <td><span class="count">@item.Message</span></td>
                                    <td><span class="count">@item.Time</span></td>
                                    <td class="">
                                        <img src="@item.InvitationStyle.ImageFilePath" style="width:10px; height:15px;">
                                    </td>
                                    <td>
                                        <span class="badge badge-complete">@item.Status</span>
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@*Breadcrumb Section End
    Shoping Cart Section Begin*@

